<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sellerslog.musicapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/image_music"
        android:label="@string/app_name"
        android:roundIcon="@drawable/image_music"
        android:supportsRtl="false"
        android:theme="@style/MyAppTheme">
        <activity
            android:name=".ActivitiesPackage.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivitiesPackage.FavoritesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivitiesPackage.ListSongsActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>